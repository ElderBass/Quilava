<h1 id="artistPageHeader" style="background-color:rgb(69,116,121);">Quilava</h1>

<h3 id="allArtistsHeader" style="background-color:rgba(69,116,121, 0.5);">All Artists</h3>

{{#each artists}}

<div class='container'>
    <img src='../assets/record.png' alt='record clipart' style="width: 300px; height: 300px;"/>
    <br>
    <h6>{{this.first_name}} {{this.last_name}}</h6>
    <p>Stage Name: {{this.stage_name}}</p>
    <p>Genre: {{this.genre}}</p>
    <p>Location: {{this.city}}</p>
</div>
<hr>
{{/each}}