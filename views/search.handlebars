<div class='container'>
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <h1>Search Results:</h1>
            <hr>
            {{#each artists}}
            <img src='{{this.image}}' alt='Artist Profile Pic' style="width: 300px; height: 300px;"/>
            <br>
            <h3>{{this.stage_name}}</h3>
            <p>Real Name: {{this.first_name}} {{this.last_name}}</p>
            <p>Genre: {{this.genre}}</p>
            <p>Location: {{this.city}}</p>
            <a href="/artist/{{this.id}}"><button class="btn btn-danger" style="background-color:#F64c72" data-id={{this.id}}>Jump to Profile</button></a>
            {{/each}}
            <hr>
        </div>
    </div>
</div>



